{
  "alertAttrs": {
    "type": "group",
    "values": [
      {
        "type": "attribute",
        "key": "bsStyle",
        "value": {
          "label": "Alert level",
          "type": "select",
          "values": [
            {
              "label": "Success",
              "value": "success"
            },
            {
              "label": "Info",
              "value": "info"
            },
            {
              "label": "Warning",
              "value": "warning"
            },
            {
              "label": "Danger",
              "value": "danger"
            }
          ]
        }
      },
      {
        "type": "attribute",
        "key": "onDismiss",
        "quote": "braces",
        "value": {
          "label": "Dismissable",
          "type": "switch",
          "on": "this.handleAlertDismiss"
        }
      },
      {
        "type": "attribute",
        "key": "dismissAfter",
        "quote": "braces",
        "value": {
          "label": "Automatically dismiss",
          "type": "switch",
          "on": "2000"
        }
      }
    ]
  }
}
