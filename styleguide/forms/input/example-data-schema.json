{
  "inputAttrs": {
    "type": "group",
    "values": [
      {
        "type": "attribute",
        "key": "type",
        "quote": "braces",
        "value": {
          "label": "Type",
          "type": "select",
          "values": [
            "text",
            "password",
            "datetime",
            "datetime-local",
            "date",
            "month",
            "time",
            "week",
            "number",
            "email",
            "url",
            "search",
            "tel",
            "color"
          ]
        }
      },
      {
        "type": "attribute",
        "key": "bsSize",
        "value": {
          "label": "Size",
          "type": "select",
          "optional": true,
          "values": [
            {
              "label": "Large",
              "value": "large"
            },
            {
              "label": "Small",
              "value": "small"
            }
          ]
        }
      },
      {
        "type": "attribute",
        "key": "bsStyle",
        "value": {
          "label": "Validation",
          "type": "select",
          "optional": true,
          "values": [
            {
              "label": "Success",
              "value": "success"
            },
            {
              "label": "Warning",
              "value": "warning"
            },
            {
              "label": "Error",
              "value": "error"
            }
          ]
        }
      },
      {
        "label": "Read Only",
        "type": "switch",
        "on": "readOnly"
      },
      {
        "label": "Disabled",
        "type": "switch",
        "on": "disabled"
      },
      {
        "label": "Show Icon in Validation State",
        "type": "switch",
        "on": "hasFeedback"
      }
    ]
  }
}
