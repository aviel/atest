{
  "barAttrs": {
    "type": "group",
    "values": [
      {
        "type": "attribute",
        "key": "bsStyle",
        "value": {
          "label": "Context",
          "type": "select",
          "optional": true,
          "values": [
            {
              "label": "Success",
              "value": "success"
            },
            {
              "label": "Info",
              "value": "info"
            },
            {
              "label": "Warning",
              "value": "warning"
            },
            {
              "label": "Danger",
              "value": "danger"
            }
          ]
        }
      },
      {
        "type": "attribute",
        "key": "now",
        "quote": "braces",
        "value": {
          "label": "%",
          "type": "number",
          "min": 0,
          "max": 100,
          "step": 1
        }
      },
      {
        "type": "attribute",
        "key": "label",
        "value": {
          "label": "Label",
          "type": "switch",
          "on": "%(percent)s%"
        }
      },
      {
        "label": "Screen Reader Only Label",
        "type": "switch",
        "on": "srOnly"
      },
      {
        "label": "Striped",
        "type": "switch",
        "on": "striped"
      },
      {
        "label": "Animated",
        "type": "switch",
        "on": "animated"
      }
    ]
  }
}
